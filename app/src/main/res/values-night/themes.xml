<resources>
    <!-- Base application theme. -->
    <style name="Theme.MeuPrimeiroApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaria_200</item>
        <item name="colorPrimaryVariant">@color/primaria_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secundaria_200</item>
        <item name="colorSecondaryVariant">@color/secundaria_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>